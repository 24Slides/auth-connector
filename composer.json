{
    "name": "24slides/auth-connector",
    "type": "library",
    "description": "",
    "keywords": [],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/24slides/auth-connector/issues",
        "source": "https://github.com/24slides/auth-connector"
    },
    "authors": [
        {
            "name": "Artem Brezhnev",
            "email": "brezzhnev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "firebase/php-jwt": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.0|^6.0|^7.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "~3.8.0",
        "phpunit/php-code-coverage": "^6.0",
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Slides\\Connector\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slides\\Connector\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "aliases": {
                "AuthService": "Slides\\Connector\\Auth\\Facades\\AuthService",
                "MandrillMail": "Slides\\Connector\\Auth\\Facades\\MandrillMail"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
